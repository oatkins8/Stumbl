<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://picsum.photos/200/300/?blur);">
  <div class="container">
    <%= link_to venue.name, venue_path(venue) %>
    <p text-center><%= venue.location %></p>
  </div>
</div>
<br>
<div class="container">
  <div class="row">
    <div class="col-12">
      <% venue.events.each do |event| %>
        <div class="card-category"> <%# need to add background image via cloudinary? %>
        <%= link_to event.name, event_path(event) %>
        </div>
      <% end %>
  </div>
</div>
