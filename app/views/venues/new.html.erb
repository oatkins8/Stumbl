

<div class="container w-75">
<h1> Add New Venue </h1>
<%= simple_form_for @venue do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
   <%= f.input :about%>
    <%= f.input :website %>
     <%= f.input :facebook %>
      <%= f.input :instagram %>
       <%= f.input :photo %>
        <%= f.input :photos %>
  <%= f.button :submit %>
<% end %>
</div>
